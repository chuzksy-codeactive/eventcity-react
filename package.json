    "node-sass-chokidar": "0.0.3",
    "nodemon": "^1.17.4",
    "normalize.css": "^7.0.0",
    "passport": "^0.4.0",
    "passport-jwt": "^3.0.1",
    "passport-local": "^1.0.0",
    "pg": "^7.4.0",
    "prop-types": "^15.6.0",
    "react": "^16.2.0",
    "react-datepicker": "^1.2.2",
    "react-day-picker": "^7.1.10",
    "react-dom": "^16.2.0",
    "react-dropzone": "^4.2.7",

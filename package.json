{
  "name": "server",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "postinstall": "npm run build",
    "testdb": "NODE_ENV=test sequelize db:migrate:undo:all --env test && sequelize db:migrate --env test && sequelize db:seed:undo:all --env test && sequelize db:seed:all --env test",
    "test": "npm run build && npm run testdb && NODE_ENV=test nyc mocha --exit  --timeout 40000 ./server/dist/test/index.test.js --recursive || true",
    "start": "node server/dist/bin/www",
    "server:dev": "nodemon --watch server --exec babel-node -- server/bin/www",
    "build": "rimraf ./server/dist/ && babel ./server --out-dir ./server/dist/ --copy-files && webpack --config ./client/config/webpack.prod.js -p",
    "test:coverage": "NODE_ENV=test npm test && nyc report --reporter=lcov --reporter=text --reporter=json || coveralls",
    "coveralls": "npm test && nyc report --reporter=text-lcov | coveralls",
    "build-css": "node-sass-chokidar ./client/src/ -o ./client/src/",
    "watch-css": "npm run build-css && node-sass-chokidar ./client/src/ -o ./client/src/ --watch --recursive",
    "start:client:dev": "webpack-dev-server --config=./client/config/webpack.dev.js",
    "only": "webpack --config ./client/config/webpack.prod.js -p"
  },
  "dependencies": {
    "axios": "^0.17.1",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-register": "^6.26.0",
    "babel-runtime": "^6.26.0",
    "bcrypt-nodejs": "0.0.3",
    "body-parser": "^1.17.2",
    "chai-http": "^3.0.0",
    "chai-passport-strategy": "^1.0.1",
    "cloudinary": "^1.10.0",
    "cookie-parser": "^1.4.3",
    "cool-ascii-faces": "^1.3.4",
    "cors": "^2.8.4",
    "css-hot-loader": "^1.3.9",
    "css-loader": "^0.28.11",
    "debug": "^2.6.9",
    "dotenv": "^5.0.1",
    "express": "^4.15.2",
    "express-validator": "^4.3.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "history": "^4.7.2",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "jade": "^1.11.0",
    "jsonwebtoken": "^7.4.3",
